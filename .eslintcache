ge\"\n              width=\"50\"\n              height=\"50\"\n            />\n            <h3>{name}</h3>\n            <p>@{nickname}</p>\n          </SidebarProfile>\n\n          <SidebarMenu>\n            <SidebarItem>\n              <SidebarLink to=\"/dashboard\">\n                Add book <PlusIcon />\n              </SidebarLink>\n            </SidebarItem>\n            <SidebarItem>\n              <SidebarLink to=\"/books\">\n                Books <span>{booksCount}</span>\n              </SidebarLink>\n            </SidebarItem>\n            <SidebarItem>\n              <SidebarLink to=\"/creations\">\n                My books <span>{createdCount}</span>\n              </SidebarLink>\n            </SidebarItem>\n            <SidebarItem>\n              <SidebarLink to=\"/favorites\">My favorites {favoritedCount}</SidebarLink>\n            </SidebarItem>\n            <SidebarItem>\n              <SidebarLink to=\"/account\">Account</SidebarLink>\n            </SidebarItem>\n          </SidebarMenu>\n          <Divider />\n          <SidebarMenu>\n            <SidebarItem>\n              <SidebarLink to=\"/help\">Help</SidebarLink>\n            </SidebarItem>\n            <SidebarItem>\n              <AnchorLink onClick={handleLogout}>Logout</AnchorLink>\n            </SidebarItem>\n          </SidebarMenu>\n        </>\n      ) : (\n        <SidebarMenu>\n          <SidebarItem>\n            <SidebarLink to=\"/books\">Books</SidebarLink>\n          </SidebarItem>\n          <SidebarItem>\n            <SidebarLink to=\"/login\">Login</SidebarLink>\n          </SidebarItem>\n          <SidebarItem>\n            <SpecialLink to=\"/sign_up\">Sign up</SpecialLink>\n          </SidebarItem>\n        </SidebarMenu>\n      )}\n      <SidebarLogo to=\"/\">\n        <img src={logo} alt=\"brand logo\" width=\"50\" />\n        Books\n      </SidebarLogo>\n    </SidebarContainer>\n  );\n};\n\nSidebar.propTypes = {\n  isOpen: PropTypes.bool.isRequired,\n  toggle: PropTypes
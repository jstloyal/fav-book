         ref={register({\n              required: {\n                value: true,\n                message: 'This field is mandatory',\n              },\n              minLength: {\n                value: 6,\n                message: 'Minimum 6 characters',\n              },\n              maxLength: {\n                value: 50000,\n                message: 'Maximum 50.000 characters',\n              },\n            })}\n          />\n          <p>{errors.description && errors.description.message}</p>\n        </div>\n        <div>\n          <input\n            name=\"author\"\n            placeholder=\"Author\"\n            ref={register({\n              required: {\n                value: true,\n                message: 'This field is mandatory',\n              },\n              minLength: {\n                value: 6,\n                message: 'Minimum of 6 characters',\n              },\n              maxLength: {\n                value: 30,\n                message: 'Maximum 30 characters',\n              },\n            })}\n          />\n          <p>{errors.author && errors.author.message}</p>\n        </div>\n        <div>\n          <input\n            name=\"genre\"\n            placeholder=\"Genre\"\n            ref={register({\n              required: {\n                value: true,\n                message: 'This field is mandatory',\n              },\n              minLength: {\n                value: 3,\n                message: 'Minimum of 3 characters required',\n              },\n              maxLength: {\n                value: 15,\n                message: 'Maximum of 15 characters',\n              },\n            })}\n          />\n          <p>{errors.genre && errors.genre.message}</p>\n        </div>\n        <div>\n          <label htmlFor=\"image\">\n            <input\n              type=\"file\"\n              name=\"image\"\n              accept=\"image/\"\n              ref={register({\n                required: {\n                  value: true,\n                  message: 'This field us mandatory',\n                },\n              })}\n            />\n            <p>{errors.image && errors.image.message}</p>\n          </label>\n        </div>\n        <div>\n          <input\n            type=\"hidden\"\n            name=\"user_id\"\n            defaultValue={user.id}\n            ref={register}\n          />\n        </div>\n        {loading ? (\n          <button type=\"submit\" disabled aria-disabled>\n            Adding book...\n          </button>\n        ) : (\n          <button type=\"submit\">Add book</button>\n        )}\n        {(error && <Error errors={error} />) || null}\n      </form>\n    </div>\n  );\n};\n\nexport default AddBookForm;\n","/home/jsloyalty/Desktop/Redux/fav-book/src/features/user/LoginForm.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/features/catalog/AllBooks.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/components/Navbar/NavElement.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/utils/date.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/features/catalog/FavoriteButton.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/features/catalog/Book.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/features/catalog/DeleteButton.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/components/Sidebar/index.js",["157","158"],"/home/jsloyalty/Desktop/Redux/fav-book/src/components/Sidebar/SidebarElements.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/components/MainContainer.styled.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/components/HeroSection/index.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/components/HeroSection/HeroSection.styled.js",[],{"ruleId":"159","replacedBy":"160"},{"ruleId":"161","replacedBy":"162"},{"ruleId":"163","severity":1,"message":"164","line":9,"column":3,"nodeType":"165","messageId":"166","endLine":9,"endColumn":10},{"ruleId":"167","severity":1,"message":"168","line":48,"column":21,"nodeType":"169","endLine":53,"end
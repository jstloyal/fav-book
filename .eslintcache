n    }\n  });\n\n  return (\n    <>\n      <Nav sidebarIsOpen={sidebarIsOpen} scrolled={scrolled}>\n        <NavContainer>\n          <MobileIcon onClick={toggle}>\n            <FaBars />\n          </MobileIcon>\n          <NavLogo to=\"/\">\n            <img src={logo} alt=\"Company brand\" width=\"40\" />\n            Books\n          </NavLogo>\n\n          <MobileIcon>\n            <FaSearch />\n          </MobileIcon>\n          <NavMenu>\n            {loggedIn ? (\n              <>\n                <li>\n                  <NavLink to=\"/books\">Books</NavLink>\n                </li>\n                <li>\n                  <ProfileAvatar onClick={toggle}>\n                    <img\n                      src=\"http://unsplash.it/30/30?gravity=center\"\n                      alt=\"Random image\"\n                      width=\"30\"\n                      height=\"30\"\n                    />\n                  </ProfileAvatar>\n                </li>\n              </>\n            ) : (\n              <>\n                <li>\n                  <NavLink to=\"/\">Go Home</NavLink>\n                </li>\n                <li>\n                  <NavLink to=\"/login\">Login</NavLink>\n                </li>\n                <li>\n                  <NavLink to=\"/sign_up\">\n                    <SpecialLink>Sign up</SpecialLink>\n                  </NavLink>\n                </li>\n              </>\n            )}\n          </NavMenu>\n        </NavContainer>\n      </Nav>\n    </>\n  );\n};\n\nNavbar.propTypes = {\n  toggle: PropTypes.func.isRequired,\n  sidebarIsOpen: PropTypes.bool.isRequired,\n};\n\nexport default Navbar;\n","/home/jsloyalty/Desktop/Redux/fav-book/src/components/Layout/Navbar/NavElement.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/components/index.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/components/Books/index.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/src/components/Home/index.js",[],"/home/jsloyalty/Desktop/Redux/fav-book/